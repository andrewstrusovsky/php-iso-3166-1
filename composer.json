{
    "name": "zhuravel/php-iso-3166-1",
    "type": "library",
    "description": "PHP ISO 3166-1 helper library",
    "license": "MIT License",
    "authors": [
        {
            "name": "Roman Zhuravel",
            "email": "zhooravell@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4 || ^7.0"
    },
    "autoload": {
        "files": [
            "./src/functions.php",
            "./src/constants.php"
        ],
        "psr-4": {
